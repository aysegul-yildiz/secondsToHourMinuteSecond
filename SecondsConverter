public class SecondsConverter
{
    public static void main(String[] args) 
    {  
        int x;
        double y;

        // define x and y values
        x = -5;
        y = ( Math.pow( x, 3 ) + 3 * Math.abs(x) + 9 ) / Math.pow( x, 2 );

       int z = (int) Math.round (y);

       // print the result
       System.out.println( "The result of the expression:" + z );        
    }
    
}
